{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "DATABASE_URL": "sqlite:///new_database.db",
    "SECRET_KEY": "secret_key_2001"
  }
}
